{
	"compilerOptions": {
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"isolatedModules": true,
		"moduleResolution": "node16",
		"preserveWatchOutput": true,
		"skipLibCheck": true,
		"noEmit": true,
		"strict": true,
		"module": "node16"
	},
	"exclude": ["node_modules"]
}
